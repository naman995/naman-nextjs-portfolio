{
  "compilerOptions": {
    "paths": {
      "@/*": ["./src/*"]
    },
    "skipLibCheck": true,
    "types": [],
    "typeRoots": ["./node_modules/@types"],
    "checkJs": false
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", ".next", "public"],
  "typeAcquisition": {
    "enable": false,
    "exclude": ["page"]
  }
}
